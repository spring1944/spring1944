		sleep 200;
		move v_barrel2 to z-axis [0] speed AA20_GUN_BACK_SPEED;
		hide v_flare2;
	}
	if(vierling_flare == 3)
	{
		show v_flare3;
		move v_barrel3 to z-axis (0-AA20_GUN_RECOIL) speed AA20_GUN_RECOIL_SPEED;
		sleep 200;
		move v_barrel3 to z-axis [0] speed AA20_GUN_BACK_SPEED;
		hide v_flare3;
	}
	if(vierling_flare == 4)
	{
		show v_flare4;
		move v_barrel4 to z-axis (0-AA20_GUN_RECOIL) speed AA20_GUN_RECOIL_SPEED;
		sleep 200;
		move v_barrel4 to z-axis [0] speed AA20_GUN_BACK_SPEED;
		hide v_flare4;
	}
	vierling_flare = vierling_flare + 1;
	if(vierling_flare > 4)
	{
		vierling_flare = 1;
	}
}

