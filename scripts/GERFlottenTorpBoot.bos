Killed(severity, corpsetype)
{
	signal SIG_DEATH;
	if (severity<99)
	{
		corpsetype = 1;
		move h1_back to z-axis (0-HULL_MOVE_DIST) speed HULL_MOVE_SPEED;
		turn h1_back to x-axis (0-HULL_LIST_ANGLE) speed HULL_LIST_SPEED;
		turn h1_front to x-axis (0-HULL_LIST_ANGLE) speed HULL_LIST_SPEED;
		wait-for-move h1_back along z-axis;
		wait-for-turn h1_front around x-axis;
		return (TRUE);
	}
	corpsetype = 2;
	explode h1_front type		SHATTER|BITMAP1;
	explode h2_front type		SHATTER|BITMAP1;
	explode h1_back type		SHATTER|BITMAP1;
	explode h2_back type		SHATTER|BITMAP1;
	return (TRUE);
}