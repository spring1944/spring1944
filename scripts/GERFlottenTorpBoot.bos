RestoreTurret()
{
	set-signal-mask SIG_DEATH|ALL_WEAPON_SIGS;
	sleep RESTORE_PERIOD;

	turn main_turret_a to y-axis <0> speed MAIN_TURRET_TURN;
	turn main_sleeve_a to x-axis <0> speed MAIN_TURRET_PITCH;
	turn main_turret_b to y-axis <0> speed MAIN_TURRET_TURN;
	turn main_sleeve_b to x-axis <0> speed MAIN_TURRET_PITCH;
	turn main_turret_c to y-axis <180> speed MAIN_TURRET_TURN;
	turn main_sleeve_c to x-axis <0> speed MAIN_TURRET_PITCH;
	turn main_turret_d to y-axis <180> speed MAIN_TURRET_TURN;
	turn main_sleeve_d to x-axis <0> speed MAIN_TURRET_PITCH;

	turn aa37_turret_1 to y-axis <0> speed AA37_TURRET_TURN;
	turn aa37_sleeve_1 to x-axis <0> speed AA37_TURRET_PITCH;
	turn aa37_turret_2 to y-axis <0> speed AA37_TURRET_TURN;
	turn aa37_sleeve_2 to x-axis <0> speed AA37_TURRET_PITCH;

	turn aa20_mount_1 to y-axis <0> speed AA20_TURRET_TURN;
	turn aa20_sleeve_1 to x-axis <0> speed AA20_TURRET_PITCH;
	turn aa20_mount_2 to y-axis <0> speed AA20_TURRET_TURN;
	turn aa20_sleeve_2 to x-axis <0> speed AA20_TURRET_PITCH;
	turn aa20_mount_3 to y-axis <0> speed AA20_TURRET_TURN;
	turn aa20_sleeve_3 to x-axis <0> speed AA20_TURRET_PITCH;
	turn aa20_mount_4 to y-axis <0> speed AA20_TURRET_TURN;
	turn aa20_sleeve_4 to x-axis <0> speed AA20_TURRET_PITCH;
	turn aa20_mount_5 to y-axis <0> speed AA20_TURRET_TURN;
	turn aa20_sleeve_5 to x-axis <0> speed AA20_TURRET_PITCH;

	turn vierling to y-axis <180> speed AA20_TURRET_TURN;
	turn v_sleeve to x-axis <0> speed AA20_TURRET_PITCH;

	turn torp_turret_a to y-axis <0> speed TL_TURRET_TURN;
	turn torp_turret_b to y-axis <0> speed TL_TURRET_TURN;
}

